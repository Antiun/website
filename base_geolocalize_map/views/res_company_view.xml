<openerp>
<data>

<record id="view_company_form" model="ir.ui.view">
    <field name="name">Add maps options</field>
    <field name="model">res.company</field>
    <field name="inherit_id" ref="base.view_company_form"/>
    <field name="arch" type="xml">
        <group string="Bank Accounts" position="before">
            <group string="Geo Localization">
                <group>
                    <button string="Geo Localize"
                            name="geo_localize"
                            colspan="2"
                            icon="gtk-apply"
                            type="object"/>
                    <field name="partner_latitude"/>
                    <field name="partner_longitude"/>
                    <field name="date_localization"/>
                </group>
                <group>
                    <field name="map_marker"/>
                    <field name="map_zoom"/>
                </group>
            </group>
        </group>
    </field>
</record>

</data>
</openerp>
